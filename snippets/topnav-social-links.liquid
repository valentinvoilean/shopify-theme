{% unless settings.display_social_icons == 'hide' %}
<div class="social_links {{ settings.display_social_icons }}">
    {% unless settings.facebook_url == blank %}
    <a class="social_link" href="{{ settings.facebook_url }}" title="Follow us on Facebook" target="_blank"><i class="fa fa-facebook"></i></a>
    {% endunless %}
    {% unless settings.twitter_url == blank %}
    <a class="social_link" href="{{ settings.twitter_url }}" title="Follow us on Twitter" target="_blank"><i class="fa fa-twitter"></i></a>
    {% endunless %}
    {% unless settings.pinterest_url == blank %}
    <a class="social_link" href="{{ settings.pinterest_url }}" title="Follow us on Pinterest" target="_blank"><i class="fa fa-pinterest"></i></a>
    {% endunless %}
    {% unless settings.googleplus_url == blank %}
    <a class="social_link" href="{{ settings.googleplus_url }}" title="Follow us on Google+" target="_blank"><i class="fa fa-google-plus"></i></a>
    {% endunless %}
    {% unless settings.instagram_url == blank %}
    <a class="social_link" href="{{ settings.instagram_url }}" title="Follow us on Instagram" target="_blank"><i class="fa fa-instagram"></i></a>
    {% endunless %}
    {% unless settings.linkedin_url == blank %}
    <a class="social_link" href="{{ settings.linkedin_url }}" title="Follow us on linkedin" target="_blank"><i class="fa fa-linkedin"></i></a>
    {% endunless %}
    {% unless settings.flickr_url == blank %}
    <a class="social_link" href="{{ settings.flickr_url }}" title="Follow us on Flickr" target="_blank"><i class="fa fa-flickr"></i></a>
    {% endunless %}
    {% unless settings.yelp_url == blank %}
    <a class="social_link" href="{{ settings.yelp_url }}" title="Look us up on Yelp" target="_blank"><i class="fa fa-yelp"></i></a>
    {% endunless %}
    {% unless settings.vimeo_url == blank %}
    <a class="social_link" href="{{ settings.vimeo_url }}" title="Follow us on Vimeo" target="_blank"><i class="fa fa-vimeo"></i></a>
    {% endunless %}
    {% unless settings.youtube_url == blank %}
    <a class="social_link" href="{{ settings.youtube_url }}" title="Follow us on YouTube" target="_blank"><i class="fa fa-youtube"></i></a>
    {% endunless %}
    {% unless settings.tumblr_url == blank %}
    <a class="social_link" href="{{ settings.tumblr_url }}" title="Come visit our blog" target="_blank"><i class="fa fa-tumblr"></i></a>
    {% endunless %}
    {% unless settings.contact_email == blank %}
    <a class="social_link" href="mailto:{{ settings.contact_email }}" title="Email us" target="_blank"><i class="fa fa-envelope-o"></i></a>
    {% endunless %}
</div>
{% endunless %}